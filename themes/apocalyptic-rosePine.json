{
  "name": "Apocalyptic Rose Pine",
  "type": "dark",
  "colors": {
    "editor.background": "#0f0c0b",
    "editor.foreground": "#e0d7c6",
    "editorCursor.foreground": "#ffa500",
    "editor.selectionBackground": "#3a2e2a",
    "editor.lineHighlightBackground": "#2b2420",
    "activityBar.background": "#1b1614",
    "sideBar.background": "#1c1614",
    "statusBar.background": "#2c2622",
    "statusBar.foreground": "#ffb347"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": { "foreground": "#7a6a5c", "fontStyle": "italic" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#ffa500" }
    },
    {
      "scope": ["keyword"],
      "settings": { "foreground": "#ff7f50" }
    },
    {
      "scope": ["function"],
      "settings": { "foreground": "#ffcc66" }
    },
    {
      "scope": ["variable", "constant"],
      "settings": { "foreground": "#d9d9d9" }
    }
  ]
}
